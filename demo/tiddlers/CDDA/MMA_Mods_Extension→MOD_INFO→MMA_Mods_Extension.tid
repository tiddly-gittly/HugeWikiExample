{
  "type": "MOD_INFO",
  "id": "MMA_Mods_Extension",
  "name": "<color_yellow>Mythical Martial Arts - Mods Extension</color>",
  "authors": [
    "Photoloss",
    "Psychodata",
    "Caring-Goat-Deity"
  ],

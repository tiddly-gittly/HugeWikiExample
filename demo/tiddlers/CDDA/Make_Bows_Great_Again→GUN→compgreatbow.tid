{
  "id": "compgreatbow",
  "copy-from": "compgreatbow",
  "type": "GUN",
  "name": {
    "str": "compound greatbow"
  },
  "min_strength": 15,
  "ranged_damage": {
    "damage_type": "stab",
    "amount": 35
  },

    "surface": "point"
  },
  "bashing": 9,
  "material": [
    "plastic",
    "steel",
    "wood"
  ],
  "looks_like": "ak47",
  "color": "brown",
  "ammo": [
    "9x39"
  ],
  "built_in_mods": [
    "pso1_scope2",
    "suppressor_vss2"
  ],
  "dispersion": 60,
  "//": "We did not have the facilities at this time to measure group size, although we understand that the accuracy is excellent out to 150 meters and our observation indicated 1-2 MOA.",
  "//2": "Assuming *total* 1.5 moa with 9x39mm SP-5",
  "durability": 8,
  "loudness": 22,
  "min_cycle_recoil": 1350,
